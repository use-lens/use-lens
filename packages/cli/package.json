{
  "name": "@use-lens/cli",
  "version": "1.0.5",
  "description": "CLI for Lens API code generation",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/use-lens/use-lens",
  "author": "Andrii Shupta <andrii.shupta@gmail.com>",
  "license": "MIT",
  "keywords": [
    "lens",
    "lens-protocol",
    "use-lens",
    "react",
    "apollo-client",
    "react-apollo",
    "urql",
    "cli"
  ],
  "private": false,
  "bin": {
    "use-lens": "./lib/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "api",
    "lib",
    ".use-lens.json",
    "lens-api.documents.graphql",
    "lens-api.schema.graphql",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "precompile": "sh copy-api.sh",
    "compile": "tsc --project tsconfig.json",
    "build": "yarn run compile"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "chalk": "^5.0.1",
    "commander": "^9.4.0",
    "graphql": "^16.6.0"
  },
  "gitHead": "9928367845fa4de18a4093be965af7433acb0ea2"
}
